<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lucas.StudentMapper">
    <select id="selectByGrade" parameterType="String" resultType="student">
        select * from t_student where grade=#{grade}
    </select>
</mapper>